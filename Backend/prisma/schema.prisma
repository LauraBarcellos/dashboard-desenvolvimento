generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model WorkItem {
  id          Int      @id
  title       String
  type        String
  status      String
  project     String
  createdAt   DateTime
  activatedAt DateTime?
  closedAt    DateTime?
}
